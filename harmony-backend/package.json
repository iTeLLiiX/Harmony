{
  "name": "harmony-backend",
  "version": "1.0.0",
  "description": "Harmony Dating App - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run type-check",
    "docker:build": "docker build -t harmony-backend .",
    "docker:run": "docker run -p 5000:5000 harmony-backend",
    "db:seed": "node dist/scripts/seed.js",
    "db:migrate": "node dist/scripts/migrate.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.37.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "socket.io": "^4.6.1",
    "twilio": "^4.7.0",
    "xss": "^1.0.14",
    "stripe": "^12.0.0",
    "redis": "^4.6.0",
    "firebase-admin": "^11.0.0",
    "sharp": "^0.32.0",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.7",
    "@types/lodash": "^4.14.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4",
    "prettier": "^2.8.0",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.12"
  },
  "keywords": [
    "dating",
    "api",
    "nodejs",
    "express",
    "mongodb",
    "harmony",
    "deutsch",
    "typescript"
  ],
  "author": "Harmony Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harmony-dating/backend.git"
  },
  "bugs": {
    "url": "https://github.com/harmony-dating/backend/issues"
  },
  "homepage": "https://harmony-dating.de"
}